{
  "name": "digital-app-portal",
  "version": "0.1.0",
  "description": "Digital Apps Portal",
  "keywords": [
    "angular2",
    "angular4",
    "webpack",
    "webpack2",
    "typescript"
  ],
  "author": "Digital Apps Team",
  "contributors": [
    "Digital Apps Team"
  ],
  "homepage": "https://github.com/esure-dev/digital-app-portal",
  "license": "LicenseRef-LICENSE",
  "private": true,
  "precommit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "build": "npm run clean:dist && cross-env NODE_ENV=production webpack --progress --profile --bail --mode production",
    "build:analyze": "cross-env ANALYZE_BUILD=true npm run build",
    "build:es": "cross-env BRAND=esure npm run build",
    "build:sw": "cross-env BRAND=sheilaswheels npm run build",
    "build:fa": "cross-env BRAND=firstalternative npm run build",
    "build:docs": "npm run clean:docs && npm run typedoc && npm run compodoc && node scripts/documentation-helper.js",
    "build:e2e-ani-off": "cross-env BRAND=esure ENDTOEND=true npm run build",
    "ci:es": "cross-env CI_BUILD=true npm run build:es && cross-env BRAND=esure npm run test",
    "ci:sw": "cross-env CI_BUILD=true npm run build:sw && cross-env BRAND=sheilaswheels npm run test",
    "ci:fa": "cross-env CI_BUILD=true npm run build:fa && cross-env BRAND=firstalternative npm run test",
    "ci:docs": "cross-env CI_BUILD=true npm run build && npm run test && npm run build:docs",
    "ci:local": "exec docker/ci-local.sh",
    "clean": "npm cache verify && npm run rimraf -- typedoc dist test-reports documentation node_modules",
    "clean-install": "npm run clean && npm install",
    "clean:dist": "npm run rimraf -- dist",
    "clean:e2e": "npm run rimraf -- src/e2e/lib test-reports/e2e",
    "clean:test": "npm run rimraf -- test-reports/unit",
    "clean:docs": "npm run rimraf -- documentation typedoc compodoc",
    "compodoc": "compodoc -p tsconfig.json --theme stripe --output compodoc",
    "cucumber": "npm-run-all -p -r server:prod:ci \"protractor:cucumber --cucumberTags={1}\" --",
    "docs": "npm run typedoc && npm run compodoc",
    "e2e": "npm run e2e:pretest && npm-run-all -p -r server:prod:ci protractor",
    "e2e:ani-off": "npm run e2e:pretest && npm-run-all -p -r server:prod:ci protractor:ani-off",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e:pretest": "npm run clean:e2e && tsc -p tsconfig.e2e.json",
    "eslint": "eslint . --ext .js",
    "eslint-fix": "eslint . --fix --ext .js",
    "lint": "npm run eslint && npm run tslint -- --project tsconfig.json --format stylish && npm run sass-lint && markdownlint --ignore node_modules ./",
    "precommit-msg": "echo 'Please wait while we do our pre-commit checks...' && exit 0",
    "pretest": "npm run lint",
    "postinstall": "npm run webdriver-update && patch-package",
    "protractor": "protractor",
    "protractor:ani-off": "protractor --params.animations=false",
    "protractor:cucumber": "protractor ./protractor.cucumber.conf.js",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "sass-lint": "sass-lint 'src/**/*.scss' -v --max-warnings 0",
    "server": "webpack-dev-server --inline --progress --mode development",
    "start": "npm run start:es",
    "start:notags": "cross-env NOTAGS=true npm run start",
    "start-dev-server": "cross-env NODE_ENV=development npm run server",
    "start:allbrands": "npm-run-all -p -r start:es start:sw start:fa",
    "start:es": "cross-env BRAND=esure npm run start-dev-server -- -- --port 4201",
    "start:sw": "cross-env BRAND=sheilaswheels npm run start-dev-server -- -- --port 4202",
    "start:fa": "cross-env BRAND=firstalternative npm run start-dev-server -- -- --port 4203",
    "server:prod:ci": "npm run start-prod",
    "start-prod": "angular-http-server --path dist --p 3000 --silent",
    "test": "npm run clean:test && karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "test:logging": "cross-env TESTS_CONSOLE_LOGGING=true npm test",
    "test:docs-coverage": "compodoc -p tsconfig.json --coverageTest",
    "tslint": "tslint",
    "typedoc": "typedoc --options typedoc.json src",
    "watch": "webpack --watch --progress --profile",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start"
  },
